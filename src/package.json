    "through2": "^2.0.3",
    "ts-loader": "^2.3.7",
    "typescript": "^2.5.3",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-delete-after-emit": "^1.0.5",
    "webpack-dev-server": "^2.9.1",
    "webpack-subresource-integrity": "^1.0.1",
    "webpack2-polyfill-plugin": "^0.0.2",
    "yargs": "^9.0.1"
  }
