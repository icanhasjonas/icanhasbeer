{
  "name": "icanhasbeer",
  "version": "1.0.0",
  "description": "icanhasbeer",
  "main": "dist/index.html",
  "scripts": {
    "dev": "webpack-dev-server --hot --inline --progress --devtool cheap-eval-source-map",
    "build": "webpack --progress",
    "dist": "webpack -p --progress",
    "dist-source-map": "webpack -p --progress --devtool source-map",
    "prod": "npm run dist",
    "clean": "rimraf ./dist/*",
    "deploy": "gulp publish",
    "publish": "npm run clean && npm run dist && npm run deploy"
  },
  "keywords": [
    "webpack"
  ],
  "author": {
    "name": "Jonas Frost",
    "email": "jonas+spam@bmbsqd.com",
    "url": "https://github.com/icanhasjonas"
  },
  "contributors": [
    {
      "name": "Pontus Madsen",
      "email": "madsen@bigsouth.sshhhhh"
    },
    {
      "name": "Ted"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/icanhasjonas/icanhasbeer"
  },
  "homepage": "https://github.com/icanhasjonas/icanhasbeer#readme",
  "deploy": {
    "aws": {
      "profile": "default",
      "s3": {
        "bucket": "site.icanhasbeer.com",
        "region": "us-east-1"
      },
      "cloudfront": {
        "distributionId": "E18PUJAGF0GPDU"
      }
    }
  },
  "prettier": {
    "semi": false,
    "tabWidth": 4,
    "useTabs": true,
    "printWidth": 120,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "printWidth": 240,
          "useTabs": false,
          "tabWidth": 2,
          "trailingComma": "none",
          "parser": "json"
        }
      },
      {
        "files": "package.json",
        "options": {
          "tabWidth": 2
        }
      }
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^3.3.7",
    "core-js": "^2.5.1",
    "debug": "^3.1.0",
    "moment": "^2.18.1",
    "tslib": "^1.7.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@types/bootstrap": "^3.3.36",
    "@types/debug": "^0.0.30",
    "@types/jquery": "^3.2.13",
    "autoprefixer": "^7.1.4",
    "aws-sdk": "^2.128.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
